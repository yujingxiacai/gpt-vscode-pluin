{
	"name": "ai-code-helper-wull",
	"displayName": "ai-code-helper",
	"description": "enhance development experience,such as AI coding/AI code review„ÄÅcomment ",
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.76.0"
	},
	"categories": ["Other"],
	"activationEvents": ["onCommand:WullCopliot.init"],
	"publisher": "shenyukang",
	"repository": {
		"type": "github",
		"url": "https://github.com/yujingxiacai/gpt-vscode-pluin.git"
	},
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "WullCopliot.init",
				"title": "Hello WullCopliot",
				"category": "WullCopliot"
			},
			{
				"command": "update-key",
				"title": "UPDATE Key",
				"category": "WullCopliot"
			},
			{
				"command": "update-prompt",
				"title": "UPDATE Prompt",
				"category": "WullCopliot"
			},
			{
				"command": "ai-comment",
				"title": "AI Comment",
				"category": "WullCopliot"
			},
			{
				"command": "ai-code-generate",
				"title": "AI Code Generate",
				"category": "WullCopliot"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "ai-comment",
					"group": "navigation"
				},
				{
					"command": "ai-code-generate",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "pnpm run package",
		"publish": "vsce package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^8.0.1",
		"@types/mocha": "^10.0.1",
		"@types/node": "16.x",
		"@types/vscode": "^1.76.0",
		"@typescript-eslint/eslint-plugin": "^5.49.0",
		"@typescript-eslint/parser": "^5.49.0",
		"@vscode/test-electron": "^2.2.2",
		"copy-webpack-plugin": "^11.0.0",
		"eslint": "^8.33.0",
		"glob": "^8.1.0",
		"mocha": "^10.1.0",
		"prettier": "2.8.7",
		"rome": "^12.1.2",
		"ts-loader": "^9.4.2",
		"typescript": "^4.9.4",
		"webpack": "^5.75.0",
		"webpack-cli": "^5.0.1"
	},
	"dependencies": {
		"gpt-3-encoder": "^1.1.4",
		"node-fetch": "^3.3.1",
		"vsce": "^2.15.0"
	}
}
